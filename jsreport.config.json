{
  "httpPort": 5488,
  "store": {
    "provider": "fs"
  },
  "blobStorage": {
    "provider": "fs"
  },
  "logger": {
    "console": {
      "transport": "console",
      "level": "debug"
    }
  },
  "allowLocalFilesAccess": true,
  "reportTimeout": 60000,
  "templatingEngines": {
    "strategy": "http-server"
  },
  "extensions": {
    "chrome-pdf": {  
	  "launchOptions": {  
	    "args": ["--no-sandbox"]  
	  } 
    },
    "authentication": {
      "cookieSession": {},
      "admin": {
        "username": "admin",
        "password": "password"
      },
      "enabled": false
    },
    "scripts": {
      "strategy": "http-server"
    }
  }
}