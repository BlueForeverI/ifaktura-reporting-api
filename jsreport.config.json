{
  "httpPort": 5488,
  "store": {
    "provider": "fs"
  },
  "blobStorage": {
    "provider": "fs"
  },
  "logger": {
    "console": {
      "transport": "console",
      "level": "debug"
    }
  },
  "allowLocalFilesAccess": true,
  "reportTimeout": 90000,
  "templatingEngines": {
    "strategy": "http-server",
    "timeout": 90000
  },
  "extensions": {
    "chrome-pdf": {  
      "launchOptions": {  
        "args": ["--no-sandbox"]  
      },
      "timeout": 90000
    },
    "authentication": {
      "cookieSession": {},
      "admin": {
        "username": "admin",
        "password": "password"
      },
      "enabled": false
    },
    "scripts": {
      "strategy": "http-server",
      "timeout": 90000
    },
    "express": {
      "renderTimeout": 90000
   }
  }
}